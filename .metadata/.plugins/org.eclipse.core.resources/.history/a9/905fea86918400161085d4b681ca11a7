package hoshi;

public class hoshi {
	public static void main (String args[]){
		int i1 = 0;
		int i2 = 0;
		int j = 0;
		String hoshi = "";
		
//		jは入力用
		j = 5;
		
		for(; i1 <= 5 ;){
			
			for(; i2 <= j;){
				
				hoshi = hoshi + "☆";
				
				
			}
			
			System.out.println(hoshi);
			i2 = 0;
			hoshi = "";
		}
		
	}
	
	
}
